<% layout('layout') -%>
<!-- Default box -->
<div class="box">
  <div class="box-header with-border">
    <h3 class="box-title">
      <% if(typeof subTitulo != "undefined" && subTitulo != "undefined") { %>
          <%= subTitulo %>
          <% } %>
     </h3>
  </div>
  <div class="box-body">
    <%- body %>
  </div><!-- /.box-body -->
</div><!-- /.box -->